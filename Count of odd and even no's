#include <stdio.h>

int main()
{
    int i, even =0, odd=0;
    for(i=0; i<=10; i++)
    {
        if(i%2==0)
        {
           even = ++even; 
        }
        else
        {
            odd = ++odd;
        }
        
    }
    printf("Even no's =%d, Odd no's= %d ", even, odd);
    return 0;
}
